<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - S-EDex365</title>
    <!-- Core Css -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Fira+Sans:400,500,600" rel="stylesheet" asp-append-version="true">
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/theme.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.4/css/jquery.dataTables.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/2.3.5/css/buttons.dataTables.min.css">
    <link rel="stylesheet" href="~/frontend/css/iconfont/tabler-icons.css">
    <link rel="stylesheet" href="~/frontend/css/style.css">
    
   

    <!-- Toastr CSS -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">

    <!--Otp Card Design CDN-->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js">
    
    
    

</head>
<body>
    <!------------------------------>
    <!-- Header Start -->
    <!------------------------------>
   @*  <header class="main-header position-fixed w-100"> *@
    <header class="main-header position-fixed w-100">
        <div class="container">
            <nav class="navbar navbar-expand-xl py-0">
                <div class="logo">
                    <a class="navbar-brand py-0 me-0" asp-action="Index" asp-controller="Home"><img src="~/images/logos/dark-logo.png" alt=""></a>
                </div>
                <a class="d-inline-block d-lg-block d-xl-none d-xxl-none  nav-toggler text-decoration-none" data-bs-toggle="offcanvas" href="#offcanvasExample" aria-controls="offcanvasExample">
                    <i class="ti ti-menu-2"></i>
                </a>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mx-auto" style="display:none">
                        <li class="nav-item">
                            <a class="nav-link" aria-current="page" href="#">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">About us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Features </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Team </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Portfolio </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="StudentUser" asp-action="Index">StudentList</a>
                        </li>
                        

                    </ul>
                    <div class="ms-auto d-flex align-items-center">
                        <a class="btn btn-primary btn-hover-secondery text-capitalize me-2" asp-action="Login" asp-controller="Auth">Sign In</a>
                        <a class="btn btn-primary btn-hover-secondery text-capitalize" asp-action="Signup" asp-controller="User">Sign Up</a>

                        <a style="display:none" class="btn btn-outline-warning btn-hover-secondery text-capitalize m-2"
                           asp-action="DownloadApk" asp-controller="APK">
                            APK
                        </a>

                    </div>
                    
                </div>
            </nav>
        </div>

        <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasExample" aria-labelledby="offcanvasExampleLabel">
            <div class="offcanvas-header">
                <div class="logo">
                    <a class="navbar-brand py-0 me-0" href="#"><img src="~/images/logos/dark-logo.png" alt=""></a>
                </div>
                <button type="button" class="btn-close text-reset  ms-auto" data-bs-dismiss="offcanvas" aria-label="Close"><i class="ti ti-x"></i></button>
            </div>
            <div class="offcanvas-body pt-0">
                <ul class="navbar-nav">
                    @* <li class="nav-item">
                        <a class="nav-link" aria-current="page" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">About us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Features </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Team </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Portfolio </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Review</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Pricing</a>
                    </li> *@

                    <br />
                    <br />
                    <br />
                    

                    <a class="btn btn-success btn-hover-secondery text-capitalize" asp-action="Login" asp-controller="Auth">Sign In</a>
                    <br />
                    <a class="btn btn-warning btn-hover-secondery text-capitalize" asp-action="Signup" asp-controller="User">Sign Up</a>

                </ul>
                <div class="login d-block align-items-center">
                    <a style="display:none" class="btn btn-primary text-capitalize w-100" href="#">contact us</a>
                </div>
            </div>
        </div>
    </header>
    <!------------------------------>
    <!-- Header End  -->
    <!------------------------------>
    <!-- Contact Section End  -->
    <!------------------------------>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    <!------------------------------>
    <!-- Footer Section Start  -->
    <!------------------------------>
    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-xxl-4 col-xxl-4 col-lg-4 col-md-12 col-sm-12 col-12">
                    <div class="footer-logo-col">
                        <a href="#"><img src="~/images/logos/dark-logo.png"></a>
                        <p class="copyrights fs-5 mb-0" style="display:none">Copyright<span id="autodate"></span></p>
                        <p class="mb-0 copyrights">© 2024 Edex365. All Rights Reserved.</p>
                        <ul class="social-icons list-unstyled mb-0 pl-0 d-flex align-items-center">
                            <li><a href="#" class="rounded-circle"><img src="~/frontend/images/footer/icon_facebook.svg"><img src="~/frontend/images/footer/icon_facebook_white.svg"></a></li>
                            <li><a href="#" class="rounded-circle"><img src="~/frontend/images/footer/icon_twitter.svg"><img src="~/frontend/images/footer/icon_twitter_white.svg"></a></li>
                            <li><a href="#" class="rounded-circle"><img src="~/frontend/images/footer/icon_google_plus.svg"><img src="~/frontend/images/footer/icon_google_plus_white.svg"></a></li>
                            <li><a href="#" class="rounded-circle"><img src="~/frontend/images/footer/icon_linkdin.svg"><img src="~/frontend/images/footer/icon_linkdin_white.svg"></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-xxl-3 col-xxl-3 col-lg-3 col-md-4 col-sm-12 col-12">
                    </div>
                <div class="col-xxl-3 col-xxl-3 col-lg-3 col-md-4 col-sm-12 col-12" style="display:none">
                    <h5 class="text-secondary fw-bold fs-4">Quick links</h5>
                    <ul class="list-unstyled mb-0 pl-0">
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">Home</a></li>
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">about us</a></li>
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">Features </a></li>
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">Team </a></li>
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">Portfolio </a></li>
                        <li><a href="#" class="text-capitalize fs-6 text-decoration-none">Pricing</a></li>
                    </ul>
                </div>
                <div class="col-xxl-5 col-xxl-5 col-lg-5 col-md-8 col-sm-12 col-12">
                    <h5 class="text-secondary fw-bold fs-4">Edex365</h5>
                    <div class="blog mb-3">
                        <p class="mb-0 fs-6">Welcome to <b>EDex365</b> , your ultimate education platform designed to bridge the gap between students and teachers in a collaborative, problem-solving environment.</p>
                        <p class="mt-2 fs-6">At <b>EDex365</b>, we believe education is more than just lectures and textbooks—it’s about empowering students to overcome challenges and fostering a community where knowledge thrives.</p>
                    </div>
                    <div class="blog mb-3">
                        <a asp-action="DownloadApk" asp-controller="APK"><img src="~/images/logos/Capture.png"></a>
                        <p style="display:none" class="mb-0 fs-6">It has survived not only centuries, that rela but also the all this among lea.</p>
                        <p style="display:none" class="mt-2 fs-6">16 Mar,2021</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!------------------------------>
    <!-- Footer Section End  -->
    <!------------------------------>
    <!------------------------------>
    <!----------- Js Libs ---------->
    <!------------------------------>
    @* <script src="~/js/jquery-3.3.1.min.js" asp-append-version="true"></script> *@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.3.0/jquery.form.min.js" asp-append-version="true"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/menu.min.js"></script>
    <script src="~/js/scrollbar.min.js"></script>
    <script src="~/js/scripts.js"></script>
    <script src="~/plugins/swiper/swiper.min.js"></script>
    <script src="~/plugins/parsley/parsley.min.js"></script>
    <script src="~/plugins/isotope/isotope.pkgd.min.js"></script>
    <script src="~/js/theme.js"></script>
    <script src="~/sitejs/changepass.js"></script>
    <script src="https://kit.fontawesome.com/e19c476714.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/1.11.4/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js" asp-append-version="true"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js" asp-append-version="true"></script>
   
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/buttons/2.3.5/js/dataTables.buttons.min.js" asp-append-version="true"></script>
    <script type="text/javascript" charset="utf8" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" asp-append-version="true"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/buttons/2.3.5/js/buttons.html5.min.js" asp-append-version="true"></script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/buttons/2.3.5/js/buttons.print.min.js" asp-append-version="true"></script>
    <script type="text/javascript" charset="utf8" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js" asp-append-version="true"></script>
    
    <script src="~/frontend/js/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
